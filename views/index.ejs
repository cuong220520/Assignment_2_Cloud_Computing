<h2>Recently Added</h2>

<div>
    <% products.forEach(product => { %> 
        <a href="/products/<%= product.id %>">
            <img height="150" width="100" src="<%= product.coverImagePath %> ">
        </a>
    <% }); %> 
</div>

<form action="/logout?_method=DELETE" method="post">
    <button type="submit">Logout</button>
</form>